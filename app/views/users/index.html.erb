<%= link_to 'New User', new_user_path%>
<h1>Listing user</h1>
<table>
	<tr>
		<td>Name</td>
		<td>Email</td>
		<td></td>
		<td></td>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.name %></td>
			<td><%= user.email %></td>
			<td><%= link_to 'Show', user %></td>
			<td><%= link_to 'Edit', edit_user_path(user)%></td>
			<td><%= link_to 'Destory', user, method: :delete, data: {confirm: 'Are you sure?'}%></td>
		</tr>		
	<% end %>
</table>
<br />

<%= link_to 'Logout', users_logout_path%>
